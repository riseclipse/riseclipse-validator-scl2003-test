import scl: 'http://www.iec.ch/61850/2003/SCL'

package scl

context IED

    -- At least one AccessPoint is required
    inv IED_at_least_one_AccessPoint_required
        (   'ERROR;'
          + 'OCL/RequiredElements/IED_at_least_one_AccessPoint_required;'
          + self.lineNumber.toString() + ';'
          + 'IED shall contain at least one AccessPoint'
        )
    :
        self.AccessPoint->notEmpty()


endpackage

