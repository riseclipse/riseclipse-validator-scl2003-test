import scl: 'http://www.iec.ch/61850/2003/SCL'

package scl

context ServiceWithMax
-- extends NONE

    -- The max attribute shall be present
    inv ServiceWithMax_max_required
        (   'ERROR;'
          + 'OCL/RequiredAttributes/ServiceWithMax_max_required;'
          + self.lineNumber.toString() + ';'
          + 'max attribute shall be present in ServiceWithMax'
        )
    :
        self.max <> null

    -- The max attribute shall be valid
    inv ServiceWithMax_max_valid
        (   'ERROR;'
          + 'OCL/RequiredAttributes/ServiceWithMax_max_valid;'
          + self.lineNumber.toString() + ';'
          + 'max attribute shall be valid in ServiceWithMax. ' + 'Current value is ' + self.max.toString()
        )
    :
        self.max <> null implies self.max >= 0


endpackage

